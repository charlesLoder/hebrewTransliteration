  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@netlify/functions": "^2.7.0",
    "@types/google-spreadsheet": "^3.3.2",
    "clean-css": "^5.3.3",
    "dotenv": "^16.4.5",
    "esbuild": "^0.21.4",
    "html-minifier": "^4.0.0",
    "netlify-cli": "^17.25.0",
    "npm-check-updates": "^16.14.20",
    "patch-package": "^8.0.0"
